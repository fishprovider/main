{
  "name": "@fishprovider/old-core",
  "version": "5.0.0",
  "type": "module",
  "scripts": {
    "lint": "eslint --cache --fix .",
    "type-check": "tsc --noEmit",
    "test": "npm run test-unit",
    "test-unit": "NODE_ENV=test NODE_OPTIONS=--experimental-vm-modules npx jest",
    "test-int": "NODE_ENV=test NODE_OPTIONS=--experimental-vm-modules doppler run --print-config -- npx jest --runInBand",
    "clean": "rm -rf dist *.tsbuildinfo",
    "build": "tsc -p tsconfig.build.json && tsc-alias -p tsconfig.build.json",
    "ci-lint": "eslint --cache --fix",
    "ci-type-check": "tsc-files --noEmit types/global.d.ts"
  },
  "dependencies": {
    "@fishprovider/utils": "*",
    "@sentry/node": "^7.73.0",
    "agenda": "^5.0.0",
    "amqplib": "^0.10.3",
    "axios": "^1.6.1",
    "compression": "^1.7.4",
    "connect-redis": "^7.1.0",
    "cors": "^2.8.5",
    "discord.js": "^14.13.0",
    "dotenv-flow": "^3.3.0",
    "expo-server-sdk": "^3.7.0",
    "express": "^4.18.2",
    "express-session": "^1.17.3",
    "firebase-admin": "^11.11.0",
    "googleapis": "~123.0.0",
    "lodash": "^4.17.21",
    "loglevel": "^1.8.1",
    "moment": "^2.29.4",
    "moment-timezone": "^0.5.43",
    "mongodb": "^6.1.0",
    "p-queue": "^7.4.1",
    "pg": "^8.11.3",
    "redis": "^4.6.10",
    "socket.io": "^4.7.2",
    "systeminformation": "^5.21.11"
  },
  "devDependencies": {
    "@types/amqplib": "^0.10.2",
    "@types/compression": "^1.7.3",
    "@types/express-session": "^1.17.8",
    "@types/pg": "^8.10.5",
    "esbuild": "^0.19.4"
  }
}