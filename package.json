{
  "name": "fishprovider",
  "version": "6.0.0",
  "private": true,
  "workspaces": [
    "packages/*",
    "packages-share/*",
    "packages-backend/*",
    "packages-frontend/*",
    "apps-backend/*",
    "apps-frontend/*"
  ],
  "type": "module",
  "engines": {
    "node": ">=16 <19"
  },
  "scripts": {
    "ci": "npm i --prefer-offline --no-audit --no-progress --legacy-peer-deps",
    "postinstall": "npx husky install",
    "lint": "npm run lint -ws --if-present",
    "type-check": "cd scripts && ./type-check.sh",
    "check-deps": "cd scripts && node check-deps.cjs || ./post-check-deps.sh",
    "check-unused-deps": "cd scripts && node check-unused-deps.cjs",
    "doctor": "npm run lint && npm run type-check && npm run check-deps && npm run check-unused-deps",
    "clean": "npm run clean -ws --if-present",
    "pm2-start-all": "pm2 startOrReload pm2.config.cjs",
    "pm2-stop-all": "pm2 stop pm2.config.cjs",
    "pm2-start": "pm2 startOrReload pm2.config.cjs --only",
    "pm2-stop": "pm2 stop pm2.config.cjs --only",
    "pm2-deploy-setup": "pm2 deploy pm2.config.cjs ${DEPLOY_ENV:-localhost} setup",
    "pm2-deploy": "pm2 deploy pm2.config.cjs ${DEPLOY_ENV:-localhost}",
    "pm2-deploy-ci": "pm2 deploy pm2.config.cjs ${DEPLOY_ENV:-localhost} exec 'source ./pm2-preload.sh; npm run ci'",
    "pm2-deploy-build": "pm2 deploy pm2.config.cjs ${DEPLOY_ENV:-localhost} exec 'source ./pm2-preload.sh; npm run build -w'",
    "pm2-deploy-start": "DOPPLER_TOKEN=$(doppler configure get token --plain) pm2 deploy pm2.config.cjs ${DEPLOY_ENV:-localhost} exec 'source ./pm2-preload.sh; npm run pm2-start'",
    "pm2-deploy-stop": "pm2 deploy pm2.config.cjs ${DEPLOY_ENV:-localhost} exec 'source ./pm2-preload.sh; npm run pm2-stop'",
    "pm2-deploy-exec": "pm2 deploy pm2.config.cjs ${DEPLOY_ENV:-localhost} exec"
  },
